package br.com.senaisp.bauru.fernanda.licao04.secao01;

public class FestaAlex {
	//constantes
			final double IMPOSTO = 0.05;
			final double GORJETA = 0.15;

	public static void main(String[] args) {
		double pessoa 1 = 10;
		double pessoa 2 = 12;
		double pessoa 3 = 9;
		double pessoa 4 = 8;		
		double pessoa 5 = 7;
		double pessoa 6 = 16;
		double pessoa 7 = 11;
		double pessoa 8 = 30;
		//constantes
		final double IMPOSTO = 0.05;
		final double GORJETA = 0.15;
		double total1 = pessoa1 * ( 1 + IMPOSTO ) * ( 1 + GORJETA );
		double total2 = pessoa2 * ( 1 + IMPOSTO ) * ( 1 + GORJETA );
		
		
	}

}
